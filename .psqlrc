\set QUIET ON

\set HISTFILE ~/.psql_history- :HOST - :DBNAME
\set HISTSIZE 2000
\set COMP_KEYWORD_CASE upper

\x
\pset linestyle unicode
\pset border 2
\pset null '[NULL]'
\set PROMPT1 '%[%033[32;1m%]%M %033[34;1m%]%n@%/%R%[%033[0m%]%# '
\set PROMPT2 '%033[33;1m >>> %033[0m%]'
\pset pager off
\pset format wrapped
\timing
\encoding unicode
\unset quiet
