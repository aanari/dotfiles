Config { font = "xft:DejaVu Sans Mono:pixelsize=12:style=Regular:dpi=96:rgba=rgb:antialias=true:hinting=true:hintslight=hintsfull"
    , bgColor = "#000000"
    , fgColor = "grey"
    , position = Top
    , lowerOnStart = True
    , commands = [ Run Weather "KPHL" ["-t","<tempF>Â°F","-L","65","-H","80","--normal","green","--high","red","--low","lightblue"] 36000
